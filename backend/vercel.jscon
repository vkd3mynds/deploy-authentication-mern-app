{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/ping",
      "dest": "index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/products/(.*)",
      "dest": "index.js"
    }
  ]
}
